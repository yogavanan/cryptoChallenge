TARGET_HEX2BASE64=hex2Base64
TARGET_FIXEDXOR=fixedXOR



all: $(TARGET_HEX2BASE64) $(TARGET_FIXEDXOR)


$(TARGET_HEX2BASE64): p1/hex2Base64.cpp
	gcc -o $(TARGET_HEX2BASE64) p1/hex2Base64.cpp

$(TARGET_FIXEDXOR): p2/fixedXOR.cpp 
	gcc -o $(TARGET_FIXEDXOR) p2/fixedXOR.cpp
	
clean:
	rm -f $(TARGET_HEX2BASE64)
	rm -r $(TARGET_FIXEDXOR)